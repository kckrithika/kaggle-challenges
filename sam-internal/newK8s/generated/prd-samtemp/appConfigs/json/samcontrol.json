{
   "apiVersion": "extensions/v1beta1",
   "kind": "Deployment",
   "metadata": {
      "labels": {
         "name": "samcontrol"
      },
      "name": "samcontrol"
   },
   "spec": {
      "replicas": 1,
      "selector": {
         "matchLabels": {
            "name": "samcontrol"
         }
      },
      "template": {
         "metadata": {
            "labels": {
               "apptype": "control",
               "name": "samcontrol"
            }
         },
         "spec": {
            "containers": [
               {
                  "env": [
                     {
                        "name": "DOCKERREGISTRY",
                        "value": "shared0-samcontrol1-1-prd.eng.sfdc.net:5000"
                     },
                     {
                        "name": "FUNNELVIP",
                        "value": "mandm-funnel-sfz.data.sfdc.net"
                     }
                  ],
                  "image": "shared0-samcontrol1-1-prd.eng.sfdc.net:5000/sam-controller:mayank.kumar-20160914_145440-cfc3f85a",
                  "name": "sam-controller"
               }
            ],
            "hostNetwork": true
         }
      }
   }
}

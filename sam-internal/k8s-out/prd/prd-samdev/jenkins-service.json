{
   "apiVersion": "v1",
   "kind": "Service",
   "metadata": {
      "labels": {
         "app": "jenkins"
      },
      "name": "jenkins",
      "namespace": "sam-system"
   },
   "spec": {
      "ports": [
         {
            "name": "jenkins-port",
            "nodePort": 39104,
            "port": 8080,
            "protocol": "TCP",
            "targetPort": 8080
         }
      ],
      "selector": {
         "name": "jenkins"
      },
      "type": "NodePort"
   }
}

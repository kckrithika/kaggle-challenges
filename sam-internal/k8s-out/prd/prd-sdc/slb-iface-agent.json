{
   "apiVersion": "extensions/v1beta1",
   "kind": "DaemonSet",
   "metadata": {
      "labels": {
         "name": "slb-iface-agent"
      },
      "name": "slb-iface-agent"
   },
   "spec": {
      "template": {
         "metadata": {
            "labels": {
               "apptype": "control",
               "daemonset": "true",
               "name": "slb-iface-agent"
            }
         },
         "spec": {
            "containers": [
               {
                  "command": [
                     "/sdn/slb-iface-agent",
                     "--path=/host/var/slb",
                     "--period=2s",
                     "--marker=20s",
                     "--exec=/sdn"
                  ],
                  "image": "ops0-artifactrepo2-0-prd.data.sfdc.net/docker-release-candidate/tnrp/sdn/hypersdn:v-0000028-45cce23f",
                  "name": "slb-iface-agent",
                  "securityContext": {
                     "privileged": true
                  },
                  "volumeMounts": [
                     {
                        "mountPath": "/host/var/slb",
                        "name": "var-slb-volume"
                     }
                  ]
               }
            ],
            "hostNetwork": true,
            "volumes": [
               {
                  "mountPath": "/var/slb",
                  "name": "var-slb-volume"
               }
            ]
         }
      }
   }
}

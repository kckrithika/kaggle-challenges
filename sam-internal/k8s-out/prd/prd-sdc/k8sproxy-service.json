{
   "apiVersion": "v1",
   "kind": "Service",
   "metadata": {
      "labels": {
         "app": "k8sproxy"
      },
      "name": "k8sproxy-service",
      "namespace": "sam-system"
   },
   "spec": {
      "ports": [
         {
            "name": "k8sproxy-port",
            "nodePort": 40000,
            "port": 5000,
            "protocol": "TCP",
            "targetPort": 5000
         }
      ],
      "selector": {
         "name": "k8sproxy"
      },
      "type": "NodePort"
   }
}

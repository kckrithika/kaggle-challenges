{
   "apiVersion": "v1",
   "kind": "Service",
   "metadata": {
      "labels": {
         "app": "estate-info"
      },
      "name": "estate-info-service",
      "namespace": "sam-system"
   },
   "spec": {
      "ports": [
         {
            "name": "estate-info-port",
            "nodePort": 40001,
            "port": 8080,
            "protocol": "TCP",
            "targetPort": 9090
         }
      ],
      "selector": {
         "name": "estate-info-server"
      },
      "type": "NodePort"
   }
}
